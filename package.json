{
  "name": "vite-gold",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "serve": "run-p serve-json-server serve-app1 serve-app2 serve-nav serve-footer serve-root",
    "serve-app1": "cd apps/app1 && npm run serve",
    "serve-app2": "cd apps/app2 && npm run serve",
    "serve-nav": "cd apps/nav && npm run serve",
    "serve-footer": "cd apps/footer && npm run serve",
    "serve-root": "cd apps/spa-root && npm run dev",
    "serve-json-server": "cd server && npm run serve"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "single-spa": "^5.9.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "workspaces": [
    ".",
    "apps/*",
    "server"
  ]
}
